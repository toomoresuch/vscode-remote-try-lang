{
    "root": true,
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "eqeqeq": "warn",
        "no-cond-assign": 0,
        "no-console": 0,
        "no-extra-semi": "warn",
        "no-unused-vars": 1,
        "semi": "warn"
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    }
}
